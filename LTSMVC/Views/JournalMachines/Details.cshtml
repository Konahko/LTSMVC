@model LTSMVC.Classes.Journals.MachineJournalsDetailsViewModel

@{
    ViewData["Title"] = "Детали Журнала";
}

<h1>Детали</h1>

<div>
    <h4>Журнал</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TriggerUser)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TriggerUser)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.State)
        </dt>
        <dd class="col-sm-10">
            @for (int i = 0; i < Model.State.Length; i++)
            {
                <p>
                    @Html.DisplayFor(model => model.State[i])
                </p>
            }
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Time)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Time)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Machine)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Machine.InvNumber)
        </dd>
    </dl>
</div>
<div>
    @*<a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |*@
    <a asp-action="Index">Назад</a>
</div>
